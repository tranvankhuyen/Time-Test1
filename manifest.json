{
    "manifest_version": 3,
    "name": "Test",
    "version": "0.1",
    "background": {
        "service_worker": "background.js"
    
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js"     : ["content.js"]
        }
    ],
    "permissions": ["storage"]
}